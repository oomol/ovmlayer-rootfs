{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  schedule: [
    '* 0-17 * * 1-5',
    '* 0-12 * * 6,7',
  ],
  automerge: true,
  automergeStrategy: 'squash',
  rangeStrategy: 'pin',
  packageRules: [
    {
      enabled: false,
      matchDepNames: [
        '/.*/',
      ],
    },
    {
      groupName: '@oomol packages',
      labels: [
        'oomol packages',
      ],
      enabled: true,
      matchDepNames: [
        '@oomol{/,}**',
      ],
    },
    {
      matchDatasources: [
        'docker',
      ],
      versioning: 'semver',
      enabled: true,
    },
    {
      matchDepNames: [
        'oocana-python-executor',
        'oocana',
      ],
      groupName: 'oocana-python',
      labels: [
        'oocana-python',
      ],
      enabled: true,
    },
  ],
  npmrcMerge: true,
}
